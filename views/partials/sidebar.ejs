<% if (typeof sidebar !== 'undefined' && Array.isArray(sidebar)) { %>
<aside class="sidebar" aria-label="Primary">
  <div class="sidebar__brand">WinBuild</div>
  <nav class="sidebar__nav">
    <% sidebar.forEach((item) => { %>
      <a class="sidebar__link" href="<%= item.href %>"><span class="sidebar__icon"><%= item.icon %></span> <%= item.text %></a>
    <% }) %>
  </nav>
</aside>
<% } %>
